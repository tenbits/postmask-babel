{
  "name": "postmask-babel",
  "version": "1.0.10",
  "description": "Babel optimizer for postmask",
  "author": "Alexander Kit (alex.kit@atmajs.com)",
  "main": "lib/optimize.js",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-standalone": "^6.10.3",
    "maskjs": ">=0.54.33"
  },
  "devDependencies": {
    "atma": "latest",
    "atma-loader-babel": "^1.0.21",
    "babel-preset-es2015": "^6.9.0",
    "maskjs": ">=0.56.1"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ]
      }
    }
  }
}
